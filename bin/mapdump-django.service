[Unit]
Description="mapdump django server"
Wants=network-online.target
After=network-online.target

[Service]
Type=Simple
ExecStart=/apps/mapdump/env/bin/gunicorn -c /apps/mapdump/bin/gunicorn.conf.py -b unix:/apps/mapdump/var/django.sock
Restart=always
Environment="PATH=/apps/mapdump/env/bin/"
WorkingDirectory=/apps/mapdump
ExecReload=/bin/kill -s HUP $MAINPID
KillMode=mixed
TimeoutSec=5
PrivateTmp=true

[Install]
WantedBy=default.target
